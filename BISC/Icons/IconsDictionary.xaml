<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes">

    <DataTemplate x:Key="CloseIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="Close" />
    </DataTemplate>
    <DataTemplate x:Key="RightArrowIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="ArrowRightBold" />
    </DataTemplate>
    <DataTemplate x:Key="SaveIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="ContentSave" />
    </DataTemplate>
    <DataTemplate x:Key="OpenIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="FolderOpen" />
    </DataTemplate>
    <DataTemplate x:Key="ChevronDoubleDownIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="ChevronDoubleDown" />
    </DataTemplate>
    <DataTemplate x:Key="AlertIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="Alert" />
    </DataTemplate>
   <DataTemplate x:Key="ServerPlusIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="ServerPlus" />
    </DataTemplate>
    <DataTemplate x:Key="BlurRadialIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="BlurRadial" />
    </DataTemplate>
    <DataTemplate x:Key="BroomIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="Broom" />
    </DataTemplate>
    <DataTemplate x:Key="Bullseye">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}"  Kind="Bullseye"/>
    </DataTemplate>
    <DataTemplate x:Key="ArrowDown">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="ArrowDown" />
     </DataTemplate>
    <DataTemplate x:Key="ArrowBottomRight">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="ArrowBottomRight" />
     </DataTemplate>
    <DataTemplate x:Key="RssIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="Rss" />
    </DataTemplate>
    <DataTemplate x:Key="LanPendingIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="LanPending" />
    </DataTemplate>
    <DataTemplate x:Key="ViewGridIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="ViewGrid" />
    </DataTemplate>
    <DataTemplate x:Key="PencilIcon">
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="Pencil" />
    </DataTemplate>
    <DataTemplate x:Key="LightbulbOn">
        <!--Лампочка-->
        <materialDesign:PackIcon Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Kind="LightbulbOn" />
    </DataTemplate>
    <Style x:Key="FlatButtonLight" BasedOn="{StaticResource MaterialDesignFlatButton}" TargetType="Button">
        <!--<Style.Resources >
            <SolidColorBrush x:Key="MaterialDesignFlatButtonClick" Color="#44111121" />
        </Style.Resources>-->
    </Style>

</ResourceDictionary>